<div class="container">
    <div class="row">
        <div class="col s3"></div>
        <div class="col s6 center-align">
            <img src="http://www.tastyburger.com/wp-content/themes/tastyBurger/images/home/img-large-burger.jpg" width="300px" class="materialboxed center-block">
            <h1>EAT-DA-BURGER</h1>
        </div>
        <div class="col s3"></div>
    </div>
<div class="row">
    <div class="col s2"></div>


    <div class="col s4">
        <ul>
            {{#each burgerObject}} 
                {{#unless this.is_eaten}}
                    <li>
                        {{this.burger_id}} + ". " + {{this.burger_name}}
                    </li>
                {{/unless}} 
            {{/each}}
        </ul>
    </div>
    <div class="col s4">
        {{!--{{#each burgers}} {{#if this.is_eaten}}
        <li>
            {{this.burger_id}} + " " + {{this.burger_name}}
        </li>
        {{/if}} {{/each}}--}}
    </div>


    <div class="col s2"></div>
</div>
    <div class="row">
        <div class="col s3"></div>


        <form class="col s6" action="/" method="POST">
            <div class="input-field">
                <textarea name="addBurger" id="burgerInputTextArea"></textarea>
                <label style="margin-left:10px" for="addBurger">Add a Burger!</label>
                <button class="btn waves-effect waves-light" type="submit">Submit
            <i class="material-icons right">send</i>
            </button>
            </div>
        </form>


        <div class="col s3"></div>
    </div>
</div>